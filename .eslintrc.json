{
    "extends": ["airbnb", "next/core-web-vitals", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "react/jsx-filename-extension": [0, { "allow": "as-needed" }],
        "react/jsx-props-no-spreading": "off"
    }
}
